{
  "name": "memo",
  "version": "1.0.0",
  "description": "一个基于react的备忘录程序-客户端",
  "private": true,
  "dependencies": {
    "egg": "^1.0.0"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "concurrently": "^3.4.0",
    "egg-bin": "^3.1.0",
    "egg-ci": "^1.5.0",
    "egg-mock": "^3.2.0",
    "eslint": "^3.18.0",
    "eslint-config-egg": "^3.2.0",
    "supertest": "^3.0.0",
    "webstorm-disable-index": "^1.1.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client":"node start-client.js",
    "server": "node index.js",
    "dev": "egg-bin dev",
